{"ast":null,"code":"import { ref } from 'vue';\nimport axios from 'axios';\nexport default {\n  name: 'UserLogin',\n  setup() {\n    const form = ref({\n      username: '',\n      password: ''\n    });\n    const onSubmit = async () => {\n      try {\n        const response = await axios.post('http://localhost:3000/api/login', form.value);\n        if (response.status === 200) {\n          alert('Login avvenuto con successo');\n        } else {\n          alert('Credenziali non valide');\n        }\n      } catch (error) {\n        console.error('Errore nel login:', error.response || error.message);\n        alert(`Errore nel login: ${error.response?.data.message || error.message}`);\n      }\n    };\n    return {\n      form,\n      onSubmit\n    };\n  }\n};","map":{"version":3,"names":["ref","axios","name","setup","form","username","password","onSubmit","response","post","value","status","alert","error","console","message","data"],"sources":["/Users/alessandromagnani/Desktop/UniversitaÌ€/applicazioni-e-servizi-web/asw-project/frontend/my-vue-app/src/components/UserLogin.vue"],"sourcesContent":["<!-- src/components/UserLogin.vue -->\n<template>\n    <div class=\"container mt-5\">\n      <h2 class=\"mb-4\">Login</h2>\n      <form @submit.prevent=\"onSubmit\" class=\"needs-validation\" novalidate>\n        <div class=\"mb-3\">\n          <label for=\"username\" class=\"form-label\">Username</label>\n          <input v-model=\"form.username\" id=\"username\" type=\"text\" class=\"form-control\" required />\n          <div class=\"invalid-feedback\">Inserisci un username valido.</div>\n        </div>\n        <div class=\"mb-3\">\n          <label for=\"password\" class=\"form-label\">Password</label>\n          <input v-model=\"form.password\" id=\"password\" type=\"password\" class=\"form-control\" required />\n          <div class=\"invalid-feedback\">Inserisci una password valida.</div>\n        </div>\n        <button type=\"submit\" class=\"btn btn-primary\">Accedi</button>\n      </form>\n    </div>\n  </template>\n  \n  <script>\n  import { ref } from 'vue';\n  import axios from 'axios';\n  \n  export default {\n    name: 'UserLogin',\n    setup() {\n      const form = ref({\n        username: '',\n        password: ''\n      });\n  \n      const onSubmit = async () => {\n        try {\n          const response = await axios.post('http://localhost:3000/api/login', form.value);\n          if (response.status === 200) {\n            alert('Login avvenuto con successo');\n          } else {\n            alert('Credenziali non valide');\n          }\n        } catch (error) {\n          console.error('Errore nel login:', error.response || error.message);\n          alert(`Errore nel login: ${error.response?.data.message || error.message}`);\n        }\n      };\n  \n      return { form, onSubmit };\n    }\n  };\n  </script>\n  \n  <style scoped>\n  /* Aggiungi stili personalizzati qui se necessario */\n  </style>\n  "],"mappings":"AAqBE,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAKA,CAAA,EAAG;IACN,MAAMC,IAAG,GAAIJ,GAAG,CAAC;MACfK,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC,CAAC;IAEF,MAAMC,QAAO,GAAI,MAAAA,CAAA,KAAY;MAC3B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,IAAI,CAAC,iCAAiC,EAAEL,IAAI,CAACM,KAAK,CAAC;QAChF,IAAIF,QAAQ,CAACG,MAAK,KAAM,GAAG,EAAE;UAC3BC,KAAK,CAAC,6BAA6B,CAAC;QACtC,OAAO;UACLA,KAAK,CAAC,wBAAwB,CAAC;QACjC;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,mBAAmB,EAAEA,KAAK,CAACL,QAAO,IAAKK,KAAK,CAACE,OAAO,CAAC;QACnEH,KAAK,CAAC,qBAAqBC,KAAK,CAACL,QAAQ,EAAEQ,IAAI,CAACD,OAAM,IAAKF,KAAK,CAACE,OAAO,EAAE,CAAC;MAC7E;IACF,CAAC;IAED,OAAO;MAAEX,IAAI;MAAEG;IAAS,CAAC;EAC3B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}